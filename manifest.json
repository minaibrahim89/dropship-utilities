{
    "name": "Dropship Utilities",
    "description": "Utilities to help with dropshipping.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "storage",
        "https://www.bing.com/shop/*",
        "https://www.google.com/*"
    ],
    "optional_permissions": [
        "http://*/", 
        "https://*/"
    ],
    "browser_action": {
        "default_icon": "icons/app-icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/app-icon.png",
        "48": "icons/app-icon.png",
        "128": "icons/app-icon.png"
    }
}