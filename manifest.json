{
    "name": "Dropship Utilities",
    "description": "Utilities to help with dropshipping.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "https://www.bing.com/shop/*",
        "https://www.google.com/*"
    ],
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_popup": "popup.html"
    }
}